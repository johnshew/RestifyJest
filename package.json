{
  "name": "restifyjest",
  "version": "1.0.0",
  "description": "Using Jest with Restify",
  "main": "lib/app.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node ./lib/app.js",
    "test": "jest --coverage  --detectOpenHandles",
    "watch": "concurrently --kill-others \"tsc -w\" \"nodemon --inspect ./lib/app.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnshew/RestifyJest.git"
  },
  "author": "johnshew@users.noreply.github.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnshew/RestifyJest/issues"
  },
  "homepage": "https://github.com/johnshew/RestifyJest#readme",
  "dependencies": {
    "restify": "^8.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/restify": "^7.2.9",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.1",
    "typescript": "^3.4.1"
  }
}
